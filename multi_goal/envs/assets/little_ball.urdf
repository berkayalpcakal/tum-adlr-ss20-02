<?xml version="0.0" ?>
<robot name="urdf_robot">
  <link name="baseLink">
  	<contact>
      <rolling_friction value="0.03"/>
      <spinning_friction value="0.03"/>
    </contact>

    <inertial>
      <origin rpy="0 0 0" xyz="0 0 0"/>
       <mass value="0.5"/>
       <inertia ixx="1" ixy="0" ixz="0" iyy="1" iyz="0" izz="1"/>
    </inertial>

    <collision>
      <origin rpy="0 0 0" xyz="0 0 0"/>
      <geometry>
	       <sphere radius="0.3"/>
      </geometry>
    </collision>
    <visual>
      <geometry>
        <sphere radius="0.3"/>
       </geometry>
       <material name="transparent_green">
         <color rgba="1 0 0 1"/>
         </material>
     </visual>
  </link>
</robot>
